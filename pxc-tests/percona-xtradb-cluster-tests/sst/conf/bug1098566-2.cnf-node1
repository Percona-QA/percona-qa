# compress+parallel+compress-stream+sst-special-dirs+encrypt3

[mysqld]
innodb_data_home_dir=/tmp/var-test/data1
innodb_log_group_home_dir=/tmp/var-test/log1

[xtrabackup]
compress
parallel=4
compress-threads=4
compact


[sst]
compressor="gzip"
streamfmt=xbstream
encrypt=3
tkey=/tmp/certs/node1.key
tcert=/tmp/certs/node1.pem
