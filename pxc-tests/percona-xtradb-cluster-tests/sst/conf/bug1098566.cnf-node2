# compress+compact+xbstream+nc+encrypt1+threads

[mysqld]
innodb_data_home_dir=/tmp/var-test/data2
innodb_log_group_home_dir=/tmp/var-test/log2

[xtrabackup]
compress
parallel=4
compress-threads=4
encrypt-threads=4
encrypt=AES256
encrypt-key=6F3AD9F428143F133FD7D50D77D91EA4


[sst]
decompressor="gzip -dc"
streamfmt=xbstream
encrypt=1
sst-special-dirs=1

