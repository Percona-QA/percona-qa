./gendirs.sh | xargs -I{} echo "cd {}; ~/t; cd .." | xargs -P20 -I{} bash -c "{}" | grep -v "no core file found" | sort -u | xargs -I{} ~/kbs REVERSE "{}" | sed 's/SIGABRT|Diagnostics_area::set_error_status|[THIS STRING IS LIKELY INCORRECT HERE (AND OFTEN SEEN), AND LIKELY NOT RELATED TO THE ISSUE. PLEASE, DO NOT ASSOCIATE THIS STRING WITH THE ISSUE:] /SIGABRT|Diagnostics_area::set_error_status|/'

